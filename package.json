{
  "name": "typescript-axios",
  "version": "1.0.0",
  "description": "使用 TypeScript 实现 axios",
  "main": "index.js",
  "scripts": {
    "ws": "webpack serve --config ./config/webpack.config.dev.js",
    "wd": "webpack --config ./config/webpack.config.prod.js",
    "ss": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "./src/server/*"
    ],
    "ext": "ts,json",
    "ignore": [
      "./src/axios/*",
      "./src/web/*"
    ],
    "exec": "ts-node ./src/server/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzkuai/typescript-axios.git"
  },
  "keywords": [
    "axios",
    "ts",
    "typescript",
    "vue3"
  ],
  "author": "zzkuai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zzkuai/typescript-axios/issues"
  },
  "homepage": "https://github.com/zzkuai/typescript-axios#readme",
  "devDependencies": {
    "@vue/compiler-sfc": "^3.1.4",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.12",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "vue-loader": "^16.3.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "vue": "^3.1.4",
    "vue-router": "^4.0.10"
  }
}
