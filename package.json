{
  "name": "ts-axios-zzkuai",
  "version": "0.1.0",
  "description": "使用 TypeScript 实现 axios",
  "main": "dist/axios.umd.js",
  "module": "dist/axios.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "ws": "webpack serve --config ./config/webpack.config.dev.js",
    "wd": "webpack --config ./config/webpack.config.prod.js",
    "ss": "nodemon",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "prepub": "npm run build",
    "pub": "sh release.sh"
  },
  "nodemonConfig": {
    "watch": [
      "./src/server/*"
    ],
    "ext": "ts,json",
    "ignore": [
      "./src/axios/*",
      "./src/web/*"
    ],
    "exec": "ts-node ./src/server/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzkuai/typescript-axios.git"
  },
  "keywords": [
    "axios",
    "ts",
    "typescript",
    "vue3"
  ],
  "author": "zzkuai",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zzkuai/typescript-axios/issues"
  },
  "homepage": "https://github.com/zzkuai/typescript-axios#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@types/qs": "^6.9.7",
    "@vue/compiler-sfc": "^3.1.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.6",
    "html-webpack-plugin": "^5.3.2",
    "lodash.camelcase": "^4.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "typedoc": "^0.23.15",
    "typescript": "^4.3.5",
    "vue-loader": "^16.3.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "qs": "^6.10.1",
    "vue": "^3.1.4",
    "vue-router": "^4.0.10"
  }
}
